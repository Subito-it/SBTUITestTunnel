{
  "name": "SBTUITestTunnelClient",
  "version": "4.0.0",
  "summary": "Enable network mocks and more in UI Tests",
  "description": "This is library for UI tests target.\n\nUse this library to easily setup an HTTP tunnel between our UI Tests cases and the app under test.\nThe tunnel allows to inject data in order to enabale network mocking.",
  "homepage": "https://github.com/Subito-it/SBTUITestTunnel",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Tomas Camin": "tomas.camin@adevinta.com"
  },
  "source": {
    "git": "https://github.com/Subito-it/SBTUITestTunnel.git",
    "tag": "4.0.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "pod_target_xcconfig": {
    "prebuild_configuration": "debug"
  },
  "libraries": "z",
  "source_files": "Pod/Client/*.{h,m}",
  "dependencies": {
    "SBTUITestTunnelCommon": [

    ]
  },
  "frameworks": "XCTest",
  "user_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(PLATFORM_DIR)/Developer/Library/Frameworks"
  }
}
